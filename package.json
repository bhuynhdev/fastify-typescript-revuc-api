{
  "name": "fastify-typescript-revuc-api",
  "version": "1.0.0",
  "description": "Node.js boilerplate using fastify & TypeScript",
  "type": "module",
  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "check:types": "tsc --noEmit",
    "build": "pnpm exec swc ./src -d build",
    "build:docker:prod": "docker build . -t my-fastify-app --build-arg APP_ENV=production",
    "start": "node build/index.js",
    "dev": "tsx watch src .env | pino-pretty --colorize",
    "pretest": "npm run build",
    "test": "vitest",
    "test:watch": "vitest -w"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yonathan06/fastify-typescript-starter.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "nodejs",
    "fastify",
    "typescript"
  ],
  "author": "Bao Huynh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yonathan06/fastify-typescript-starter/issues"
  },
  "homepage": "https://github.com/yonathan06/fastify-typescript-starter#readme",
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/multipart": "^7.7.3",
    "@fastify/swagger": "^8.9.0",
    "@fastify/swagger-ui": "^1.9.3",
    "@fastify/type-provider-typebox": "^3.5.0",
    "@prisma/client": "5.2.0",
    "@sinclair/typebox": "^0.28.20",
    "@tidyjs/tidy": "^2.5.2",
    "ajv": "^8.12.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "env-schema": "^5.2.0",
    "fast-csv": "^4.3.6",
    "fastify": "^4.22.2",
    "fastify-plugin": "^3.0.1",
    "mysql2": "^3.6.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.82",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.17.13",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "esbuild": "^0.14.54",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.8.8",
    "prisma": "^5.2.0",
    "tsx": "^3.12.8",
    "typescript": "^4.9.5",
    "vitest": "^0.25.8"
  }
}
